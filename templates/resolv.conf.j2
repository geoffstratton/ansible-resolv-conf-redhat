#
# {{ ansible_managed }}
#
{% if resolv_conf_search| length > 0 %}
search {{ resolv_conf_search | join(' ') }}
{% endif %}
{% if resolv_conf_domain| length > 0 %}
domain {{ resolv_conf_domain }}
{% endif %}
{% for item in resolv_conf_nameserver %}
nameserver {{ item }}
{% endfor %}
{% if resolv_conf_options| length > 0 %}
options {{ resolv_conf_options | join(' ') }}
{% endif %}
